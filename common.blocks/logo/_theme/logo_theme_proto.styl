.logo_theme_proto
{
    display: inline-block;
    background-repeat: no-repeat;
    background-position: center;
    background-size: contain;

    &.logo_view_full
    {
        background-image: url('../_view/logo_view_full.svg');

        width: 300px;
        height: 150px;
    }

    &.logo_view_cut
    {
        background-image: url('../_view/logo_view_cut.svg');

        width: 40px;
        height: 40px;
    }
}
