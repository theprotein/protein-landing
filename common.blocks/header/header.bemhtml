block('header')(
    js()(true),
    tag()('header'),
    content()(function() {
        return {
            elem: 'content',
            content: [
                {
                    block: 'menu',
                    mix: { block: this.block, elem: 'menu' },
                    content: [
                        {
                            block: 'menu-item',
                            mods: { type: 'link', 'scroll-to': true },
                            mix: {
                                block: this.block,
                                elem: 'logo'
                            },
                            content: {
                                block: 'link',
                                url: '#protein',
                                content: {
                                    block: 'logo'
                                }
                            }
                        },
                        [
                            {
                                url: '#features',
                                text: 'Features'
                            },
                            {
                                url: '#dates',
                                text: 'Timeline'
                            },
                            {
                                url: '#pricing',
                                text: 'Pricing'
                            },
                            {
                                url: '#pre-order',
                                text: 'Sign Up'
                            }
                        ].map(function(item) {
                            return {
                                block: 'menu-item',
                                mods: { type: 'link', 'scroll-to': true },
                                content: {
                                    block: 'link',
                                    mods: { pseudo: true, theme: 'on-color' },
                                    url: item.url,
                                    content: item.text
                                }
                            };
                        })
                    ]
                }
            ]
        };
    })
);
