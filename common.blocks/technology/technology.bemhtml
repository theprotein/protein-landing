block('technology')(
    js()(true),
    tag()('a'),
    attrs()(function() {
       return {
            href: this.ctx.url || ''
       };
    }),
    content()(function() {
        this.ctx.icon && (this.ctx.icon.mix = { block: this.block, elem: 'icon' });
        return [
            this.ctx.icon,
            {
                elem: 'text',
                content: this.ctx.text
            }
        ];
    }),
    elem('sup').tag()('sup'),
    elem('text').tag()('span')
);
