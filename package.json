{
  "private": true,
  "name": "protein-landing",
  "version": "0.0.1",
  "description": "Protein.io landing page",
  "homepage": "http://theprotein.io",
  "author": "Anton Winogradov <winogradovaa@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:theprotein/protein-landing.git"
  },
  "dependencies": {
    "body-parser": "^1.9.0",
    "bower-npm-install": "^0.5.9",
    "cookie-parser": "^1.3.3",
    "enb-bem-node-specs": "^0.0.4",
    "express": "^4.9.5",
    "express-favicon": "^1.0.1",
    "express-session": "^1.9.0",
    "fs-extra": "^0.12.0",
    "json-stringify-safe": "^5.0.0",
    "lodash": "^2.4.1",
    "nconf": "^0.7.1",
    "parseurl": "^1.3.0",
    "recluster": "^0.4.0",
    "request": "^2.53.0",
    "serve-static": "^1.8.0",
    "vow": "^0.4.5",
    "vow-fs": "0.3.4",
    "winston": "^0.8.1"
  },
  "devDependencies": {
    "bem-naming": "0.5.0",
    "bh": "3.2.3",
    "borschik-tech-cleancss": "1.0.3",
    "bower": "1.3.12",
    "bower-npm-install": "~0.5.4",
    "browser-sync": "^2.0.0-rc9",
    "chai": "^2.1.0",
    "csscomb": "~3.0.3",
    "enb": "^0.15.0",
    "enb-autoprefixer": "^0.2.0",
    "enb-bem-docs": "^0.7.1",
    "enb-bem-examples": "^0.5.9",
    "enb-bem-specs": "^0.5.3",
    "enb-bem-techs": "^1.0.0",
    "enb-bem-tmpl-specs": "^0.6.1",
    "enb-bemxjst": "^1.3.3",
    "enb-bh": "^0.4.1",
    "enb-borschik": "^1.4.0",
    "enb-diverse-js": "^0.1.0",
    "enb-magic-platform": "^0.4.1",
    "enb-modules": "^0.2.0",
    "enb-stylus": "^1.2.0",
    "gemini": "^0.9.5",
    "git-hooks": "~0.0.6",
    "http-server": "^0.7.2",
    "istanbul": "git://github.com/dragn/istanbul",
    "jscs": "1.8.1",
    "jscs-bem": "^0.1.2",
    "jshint": "2.5.11",
    "jshint-groups": "0.6.2",
    "livereload": "^0.3.6",
    "mocha": "^2.1.0",
    "node-inspector": "^0.8.3",
    "nodemon": "^1.3.6",
    "should": "^5.0.1",
    "svgo": "0.5.0"
  },
  "scripts": {
    "start": "npm run dev",
    "prepare-configs": "ln -snf configs/${NODE_ENV:-development}/borschik .borschik",
    "make": "npm run prepare-configs && ./node_modules/.bin/enb make --no-cache",
    "dev": "./node_modules/.bin/enb make && node server.bundles/index/index.node.js",
    "stage": "YENV=stage NODE_ENV=stage BEM_ENV=stage node ./configs/stage/index.js",
    "production": "YENV=production NODE_ENV=production BEM_ENV=production && YENV=production NODE_ENV=production BEM_ENV=production node ./configs/production/index.js",
    "deps": "bower i",
    "compiled": "find . -iname '*.styl' | xargs stylus",
    "build": "npm run build-${TEST_SCOPE:-all}",
    "build-all": "npm run deps && magic make desktop.examples desktop.tests desktop.docs touch.examples touch.tests touch.docs *.bundles/*",
    "build-specs": "npm run deps",
    "build-tmpls": "npm run deps",
    "build-gemini": "npm run deps && magic make desktop.tests",
    "test": "npm run build && npm run ${TEST_SCOPE:-test-all}",
    "test-all": "npm run lint && npm run unit && npm run test-tmpls && npm run gemini",
    "test-specs": "./node_modules/.bin/magic run specs",
    "test-tmpls": "./node_modules/.bin/magic run tmpl-specs",
    "lint": "jshint-groups && jscs .",
    "specs": "npm run test-specs",
    "tmpls": "npm run test-tmpls",
    "unit": "npm run test-specs",
    "coverage": "ISTANBUL_COVERAGE=yes npm run specs && istanbul report --include coverage.json html text && istanbul check-coverage --include coverage.json",
    "postinstall": "npm run deps && enb make"
  },
  "engines": {
    "node": ">=0.10"
  }
}
